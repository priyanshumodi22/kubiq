# Development Environment Configuration
# This file is used when NODE_ENV=development (default)
# Git ignored - safe for local credentials

# Server Configuration
PORT=3001
NODE_ENV=development
BACKEND_DNS=http://localhost:3001
BACKEND_CONTEXT_PATH=

# Service Configuration
# Local development uses ./config/services.ini
SERVICES_CONFIG_PATH=./config/services.ini
POLL_INTERVAL=30000
REQUEST_TIMEOUT=5000

# Data Persistence
ENABLE_PERSISTENCE=true
DATA_DIR=./data
SNAPSHOT_INTERVAL=300000

# Health Check Configuration
MAX_HISTORY_SIZE=100
HEALTH_CHECK_TIMEOUT=5000

# CORS Configuration
FRONTEND_DNS=http://localhost:3000
CORS_ORIGIN=http://localhost:3000

# Keycloak OIDC Configuration
KEYCLOAK_ENABLED=true
KEYCLOAK_URL=https://Keycloak-dns/auth
KEYCLOAK_REALM=realm-name
KEYCLOAK_CLIENT_ID=kubiq

