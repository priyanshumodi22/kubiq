VITE_BACKEND_DNS=
VITE_BACKEND_CONTEXT_PATH=/kubiq-api
VITE_FRONTEND_BASE_PATH=/kubiq
VITE_APP_TITLE=Kubiq Dashboard
VITE_POLL_INTERVAL=15000
VITE_USE_SSE=true

